{
    "name": "dansmaculotte/flysystem-cloudinary",
    "description": "Flysystem adapter for Cloudinary",
    "license": "MIT",
    "keywords": ["cloudinary", "flysystem"],
    "require": {
        "php": "^8.0",
        "cloudinary/cloudinary_php": "^2.6",
        "league/flysystem": "^3.0"
        
    },
    "autoload": {
        "psr-4": {
            "DansMaCulotte\\Flysystem\\Cloudinary\\": "src/",
            "DansMaCulotte\\Flysystem\\Cloudinary\\Test\\":"tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "phpunit/php-code-coverage": "^9.2",
        "vlucas/phpdotenv": "^5.4",
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.7"
    },
    "authors": [{
        "name": "carlosocarvalho",
        "email": "carlosocarvalho.com.br@gmail.com",
        "homepage": "https://carlosocarvalho.com.br",
        "role": "Owner"
    },
    {
      "name": "Romain Touz√©",
      "email": "romain@dansmaculotte.fr"
    }],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "format": "@php vendor/bin/php-cs-fixer fix --config .php_cs.php"
    }
}
